<%= form_for [@project, @milestone] do |f| %>

	<%= render 'shared/error_messages', object: f.object %>


	<%= f.text_field :title, :autofocus => "autofocus" %>
	<%= f.label :title %>

	<%= f.text_field :due_date %>
	<%= f.label :due_date %>

	<%= f.text_field :goal%>
	<%= f.label :goal %>
	
	<%= f.text_area :desc %>
	<%= f.label :desc %>

	<%= f.submit %>
<% end %>
